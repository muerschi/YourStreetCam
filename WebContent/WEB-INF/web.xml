<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">

  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>servlets.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PictureList</servlet-name>
    <servlet-class>servlets.PictureList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Administration</servlet-name>
    <servlet-class>servlets.Administration</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserEdit</servlet-name>
    <servlet-class>servlets.UserEdit</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CameraEdit</servlet-name>
    <servlet-class>servlets.CameraEdit</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>CameraToUser</servlet-name>
    <servlet-class>servlets.CameraToUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PictureList</servlet-name>
    <url-pattern>/piclist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Administration</servlet-name>
    <url-pattern>/administration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserEdit</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CameraEdit</servlet-name>
    <url-pattern>/camera</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CameraToUser</servlet-name>
    <url-pattern>/cameraToUser</url-pattern>
  </servlet-mapping>
</web-app>